            android:id="@+id/imageView230"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView229"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/knopka2" />

        <ImageView
            android:id="@+id/imageView210"
            android:layout_width="90dp"
            android:layout_height="60dp"
            app:srcCompat="@drawable/zvezda"
            android:layout_marginStart="13dp"
            android:layout_marginBottom="3dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

        <ImageView
            android:id="@+id/imageView22"
            android:layout_width="67dp"
            android:layout_height="67dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView210"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/zadania"
            android:layout_marginStart="20dp"
            android:layout_marginBottom="3dp"/>

        <ImageView
            android:id="@+id/imageView23"
            android:layout_width="105dp"
            android:layout_height="60dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView22"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/chat"
            android:layout_marginStart="10dp"/>

        <ImageView
            android:id="@+id/imageView24"
            android:layout_width="60dp"
            android:layout_height="60dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView229"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/bot"
            android:layout_marginStart="28dp"/>

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
