        app:layout_constraintTop_toTopOf="@+id/imageViewProfile"
        app:srcCompat="@drawable/rectangle228" />

    <ImageView
        android:id="@+id/imageView49"
        android:layout_width="289dp"
        android:layout_height="71dp"
        android:layout_marginBottom="32dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView491"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/kvadrat_profile228" />

    <ImageView
        android:id="@+id/imageView491"
        android:layout_width="289dp"
        android:layout_height="71dp"
        android:layout_marginBottom="32dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView4911"
        app:layout_constraintEnd_toEndOf="parent"
