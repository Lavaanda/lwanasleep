    <TextView
        android:id="@+id/textViewVxod1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/tiny5regular"
